{
  "name": "protractor-http-client",
  "version": "1.0.4",
  "description": "HTTP client to be used in protractor tests",
  "keywords": [
    "protractor",
    "http",
    "rest",
    "client",
    "test",
    "selenium",
    "webdriver"
  ],
  "homepage": "https://github.com/marcodave/protractor-http-client",
  "scripts": {
    "compile": "rm -rf dist && tsc",
    "pretest": "npm run compile",
    "test": "webdriver-manager update --chrome=false --gecko=false && protractor dist/test/protractor.conf.js",
    "prestart": "node server/initDB.js",
    "start": "json-server --port 5000 --watch server/database.json",
    "start-auth": "node server/server.js"
  },
  "dependencies": {
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/request": "2.47.0",
    "@types/jasminewd2": "2.x",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "^4",
    "typescript": "^2.7",
    "json-server": "^0.12.2",
    "jsonwebtoken": "^8.2.1"
  },
  "author": "Marco Dalla Vecchia <mdallav@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcodave/protractor-http-client.git"
  },
  "main": "dist/http-client.js"
}
